{
  "name": "kickoff",
  "version": "1.0.0",
  "description": "Faster feature delivery by guiding agents to work like contributors",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsx scripts/build.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*\" --ignore-unknown",
    "deploy:codex": "npm run build --silent && tsx scripts/deploy-codex.ts",
    "test": "echo \"No tests yet\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grobakh/kickoff.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/grobakh/kickoff/issues"
  },
  "homepage": "https://github.com/grobakh/kickoff#readme",
  "config": {
    "promptPrefix": "kickoff",
    "codexPromptDir": "~/.codex/prompts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.3",
    "tsx": "^4.1.3",
    "typescript": "^5.3.3"
  }
}
